{
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports":"explicit"
        }
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "editor.fontWeight": "normal",
    "github.copilot.editor.enableAutoCompletions": false,
    "cSpell.words": [
        "автоматического",
        "введите",
        "выключения",
        "добавьте",
        "запуска",
        "используется",
        "исследованиях",
        "команда",
        "команду",
        "консоли",
        "контейнера",
        "контейнере",
        "курса",
        "момент",
        "например",
        "образа",
        "папки",
        "подключением",
        "портов",
        "после",
        "проброса",
        "Проект",
        "проекте",
        "прохождения",
        "Развертывание",
        "сборки",
        "сервера",
        "создан",
        "создания",
        "стадии",
        "стадия",
        "текущий",
        "удаления",
        "флаг",
        "mlops",
        "uvicorn"
    ]
}