{
  "name": "frontend",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/ --max-warnings 0 && prettier --check .",
    "format": "prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src/",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test --workers 4",
    "test:unit": "vitest run",
    "test:pl-junit": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=junit-integration.xml playwright test --workers 4 --reporter=junit",
    "test:vt-junit": "vitest run --reporter=junit",
    "pre-commit": "pre-commit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5npm",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.15.19",
    "@mui/styled-engine": "^5.14.20",
    "@mui/x-date-pickers": "^6.18.5",
    "@mui/x-tree-view": "^6.17.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "material-react-table": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-number-format": "^5.3.3",
    "react-router-dom": "^6.21.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.0.3",
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "prettier": "^3.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.4.0",
    "eslint-config-standart": "^0.2.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.3.0",
    "vitest": "^2.1.4",
    "@playwright/test": "^1.28.1",
    "pre-commit": "^1.2.2",
    "vite": "^5.0.8"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
