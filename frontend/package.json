{
  "name": "frontend",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/ --max-warnings 0 && prettier --check .",
    "format": "prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src/",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test --workers 4",
    "test:unit": "vitest run",
    "test:pl-junit": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=junit-integration.xml playwright test --workers 4 --reporter=junit",
    "test:vt-junit": "vitest run --reporter=junit",
    "pre-commit": "pre-commit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/styled-engine": "^7.0.2",
    "@mui/x-date-pickers": "^7.28.1",
    "@mui/x-tree-view": "^7.28.1",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "material-react-table": "^3.2.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^7.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.51.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.0",
    "eslint": "^9.24.0",
    "eslint-config-standart": "^0.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^15.13.0",
    "pre-commit": "^1.1.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.1",
    "vitest": "^3.1.1"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
