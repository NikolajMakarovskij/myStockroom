{
  "name": "frontend",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/ --max-warnings 0 && prettier --check .",
    "format": "prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src/",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test --workers 4",
    "test:unit": "vitest run",
    "test:pl-junit": "PLAYWRIGHT_JUNIT_OUTPUT_NAME=junit-integration.xml playwright test --workers 4 --reporter=junit",
    "test:vt-junit": "vitest run --reporter=junit",
    "pre-commit": "pre-commit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.2.1",
    "@mui/styled-engine": "^6.2.1",
    "@mui/x-date-pickers": "^7.23.2",
    "@mui/x-tree-view": "^7.23.2",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "material-react-table": "^2.13.3",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.1",
    "react-number-format": "^5.4.2",
    "react-router-dom": "^6.28.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.49.1",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-config-standart": "^0.2.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.13.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "resolutions": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
